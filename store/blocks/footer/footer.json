{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-desktop",
      "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#Products",
      "vtex.menu@2.x:menu#footer-clothing",
      "vtex.menu@2.x:menu#footer-decoration",
      "vtex.menu@2.x:menu#footer-bags",
      "footer-spacer",
      "social-networks"
    ],
    "props": {
      "blockClass": "menu-row",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },
  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/vtexonline/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/vtextruecloud/"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/vtexonline"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/user/VTEXTV"
        }
      ]
    }
  },
  "flex-layout.row#footer-2-desktop": {
    "children": [
      "flex-layout.row#pagamento",
      "flex-layout.row#seguranca",
      "flex-layout.row#developer"
    ],
    "props": {
      "blockClass": "payment-methods"
    }
  },
  "flex-layout.row#developer": {
    "children": [
      "rich-text#developer",
      "image#developer"
    ],
    "props": {
    "blockClass":"bloco-developer"
    }
  },
  "rich-text#developer": {
    "props": {
      "text": "DESENVOLVIMENTO",
      "blockClass": "footer"
    }
  },
  "image#developer": {
    "props": {
      "src": "/arquivos/Logo_4maker_v1.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },
  "flex-layout.row#seguranca": {
    "children": [
      "rich-text#seguranca",
      "flex-layout.col#selo"
    ]
  },
  "flex-layout.col#selo": {
    "children": [
      "image#seguranca2",
      "image#seguranca"
    ]
  },
  "rich-text#seguranca": {
    "props": {
      "text": "SEGURANÇA",
      "blockClass": "footer"
    }
  },
  "image#seguranca": {
    "props": {
      "src": "/arquivos/selo1.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },

  "image#seguranca2": {
    "props": {
      "src": "/arquivos/selo2.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },
  "flex-layout.row#pagamento": {
    "children": [
      "rich-text#pagamento",
     "flex-layout.col#selo-pag"
    ]
  },
  "flex-layout.col#selo-pag": {
    "children": [
      "image#pagamento-amex",
      "image#pagamento-elo",
      "image#pagamento-master-card",
      "image#pagamento-visa",
      "image#pagamento-diners",
      "image#pagamento-hipercard"
    ]
  },
  "rich-text#pagamento": {
    "props": {
      "text": "Pagamento",
      "blockClass": "footer"
    }
  },
  "image#pagamento-amex": {
    "props": {
      "src": "arquivos/Amex.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },
  "image#pagamento-elo": {
    "props": {
      "src": "arquivos/elo.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },
  
  "image#pagamento-master-card": {
    "props": {
      "src": "arquivos/elo.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },
  "image#pagamento-visa": {
    "props": {
      "src": "arquivos/elo.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },
  "image#pagamento-diners": {
    "props": {
      "src": "arquivos/elo.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
