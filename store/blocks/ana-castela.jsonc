        }
    },
    "rich-text#description-bloco-3": {
        "props": {
            "text": "Apenas as 100 primeiras inscritas vão receber um presskit com uma peça exclusiva da coleção On The Road.",
            "blockClass": [
                "title-bloco-description-3",
                "title-bloco-3-1"
            ]
        }
    },
    "rich-text#title-bloco-3-2": {
        "props": {
            "text": "## 2. Crie seu conteúdo!",
            "blockClass": [
                "title-bloco-3-0",
                "title-bloco-description-3-2"
            ]
        }
    },
    "rich-text#description-bloco-3-2": {
        "props": {
            "text": "Inspire-se na foto da Ana Castela e recrie sua própria versão da nossa campanha.",
